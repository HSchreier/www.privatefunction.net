{
  "name": "Privatefunction",
  "version": "2.3.0",
  "type": "module",
  "description": "Personal Website",
  "homepage": "https://privatefunction.net",
  "repository": {
    "type": "git",
    "url": "https://github.com/HSchreier/www.privatefunction.net.git"
  },
  "publishConfig": {
    "registry": "https://github.com/HSchreier/www.privatefunction.net"
  },
  "bugs": {
    "url": "https://github.com/HSchreier/www.privatefunction.net/issues",
    "email": "contact@privatefunction.net"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "https-preview": "serve dist",
    "prepare": "husky install && cp -n env/.shared .env &",
    "prettier:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "lint:check": "eslint --max-warnings=0 \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "ts:check": "tsc --noEmit",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^5.15.0",
    "@mui/lab": "5.0.0-alpha.156",
    "@mui/material": "^6.0.1",
    "@mui/system": "^5.15.0",
    "@mui/utils": "^5.15.0",
    "@react-three/drei": "^9.111.5",
    "@react-three/fiber": "^8.17.6",
    "@tsparticles/confetti": "^3.5.0",
    "@tsparticles/engine": "^3.5.0",
    "@tsparticles/react": "^3.0.0",
    "@tsparticles/slim": "^3.5.0",
    "gh-pages": "^6.1.1",
    "is-mobile": "^4.0.0",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-helmet-async": "^2.0.3",
    "react-hotkeys-hook": "^4.4.1",
    "react-parallax-tilt": "^1.7.251",
    "react-router-dom": "^6.21.0",
    "recoil": "^0.7.7",
    "simplex-noise": "^4.0.3",
    "three": "^0.168.0",
    "three-stdlib": "^2.33.0",
    "typewriter-effect": "^2.21.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "https-localhost": "^4.7.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.8",
    "prettier": "3.1.1",
    "typescript": "^5.5.4",
    "vite": "^5.0.9",
    "vite-plugin-pwa": "^0.17.4",
    "vitest": "^1.1.3"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --max-warnings=0"
    ]
  },
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": [
      "warn"
    ]
  },
  "author": {
    "name": "Helmut Schreier",
    "email": "helle@privatefunction.net",
    "url": "https://privatefunction.net/"
  },
  "keywords": [
    "react",
    "pwa",
    "starter-kit",
    "vite"
  ]
